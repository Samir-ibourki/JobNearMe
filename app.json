{
  "expo": {
    "name": "JobNearMe",
    "slug": "JobNearMe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",

    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A84FF"
    },

    "android": {
      "icon": "./assets/logo.png",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0A84FF"
        }
      ]
    ]
  }
}
