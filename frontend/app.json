{
  "expo": {
    "name": "JobNearMe",
    "slug": "JobNearMe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "../frontend/assets/logo1.png",
    "scheme": "jobnearme",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.samir_ibourki.JobNearMe"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-asset",
      "expo-web-browser"
    ],
    "platforms": ["android"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "aa0477dd-b481-4e8c-b0d3-35a3b751350e"
      }
    }
  }
}
