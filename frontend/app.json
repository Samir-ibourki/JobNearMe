{
  "expo": {
    "name": "JobNearMe",
    "slug": "JobNearMe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "../frontend/assets/logo1.png",
    "scheme": "jobnearme",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.samir_ibourki.JobNearMe",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCT40E6wtFMTrmOJSPlvWDt25DJu8pQGTQ"
        }
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-asset",
      "expo-web-browser",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow JobNearMe to access your location to show nearby jobs."
        }
      ],
      "expo-font"
    ],
    "platforms": [
      "android"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "aa0477dd-b481-4e8c-b0d3-35a3b751350e"
      }
    }
  }
}
